<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="sample.GameCode"
          stylesheets="/sample/sample.css"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10" >

    <Button GridPane.rowIndex ="4" text="CLICK" GridPane.columnIndex="1" fx:id="a" onAction="#increment"/>
    <Button GridPane.rowIndex="5" text="HighScores" GridPane.columnIndex="1" fx:id="b" onAction="#switchPg" visible="false"/>

    <Label GridPane.rowIndex="3" GridPane.columnIndex="1" fx:id="score" text="0"/>
</GridPane>
